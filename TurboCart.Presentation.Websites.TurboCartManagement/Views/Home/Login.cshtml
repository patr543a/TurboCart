@{
    ViewData["Title"] = "Login";
}

<h1>Log ind</h1>

<div class="form-signin">
    <form>
        <div class="form-floating mb-3">
            <input type="email" class="form-control" id="floatingInput" placeholder="Brugenavn">
            <label for="floatingInput">Brugenavn</label>
        </div>
        <div class="form-floating mb-3">
            <input type="password" class="form-control" id="floatingPassword" placeholder="Kodeord">
            <label for="floatingPassword">Kodeord</label>
        </div>

        <button class="btn btn-lg btn-primary" type="submit">Log ind</button>
    </form>
</div>